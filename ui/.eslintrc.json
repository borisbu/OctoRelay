{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "airbnb-typescript/base",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "project": true
  },
  "plugins": [
    "@typescript-eslint",
    "prettier",
    "import"
  ],
  "rules": {
  },
  "overrides": [
    {
      "files": ["*.config.*ts", "mocks/*.ts"],
      "rules": {
        "import/no-extraneous-dependencies": "off"
      }
    }
  ]
}
